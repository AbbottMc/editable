{
  "name": "@editablejs/breaker",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts",
    "dev": "tsup src/index.ts --sourcemap --format esm,cjs --watch --dts",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{json,ts,tsx,js,jsx}\"",
    "clean": "rimraf .turbo && rimraf dist && rimraf node_modules "
  },
  "dependencies": {
    "tiny-inflate": "^1.0.3",
    "unicode-trie": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node-fetch": "^2.6.1",
    "@types/unicode-trie": "^2.0.0",
    "eslint-config-editable": "workspace:*",
    "jest": "^27.5.1",
    "jest-presets": "workspace:*",
    "node-fetch": "^2.6.7",
    "prettier-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^7.x",
    "typescript": "^5.x"
  },
  "lint-staged": {
    "src/**/*.{json,ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  }
}
