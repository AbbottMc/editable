{
  "name": "@editablejs/plugin-toolbar",
  "version": "1.0.2",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.mjs"
      },
      "require": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "inline": [
        "dist/inline/index.d.ts"
      ],
      "side": [
        "dist/side/index.d.ts"
      ],
      "slash": [
        "dist/slash/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "babelMacros": {
    "twin": {
      "css": "@emotion/css",
      "styled": "@emotion/css",
      "global": {
        "import": "injectGlobal",
        "from": "@emotion/css"
      }
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{json,ts,tsx,js,jsx}\"",
    "clean": "rimraf .turbo && rimraf dist && rimraf node_modules "
  },
  "devDependencies": {
    "@types/lodash.debounce": "4.0.7",
    "@editablejs/editor": "workspace:*",
    "@editablejs/models": "workspace:*",
    "eslint-config-editable": "workspace:*",
    "jest": "^27.5.1",
    "jest-presets": "workspace:*",
    "prettier-config": "workspace:*",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tsconfig": "workspace:*",
    "tsup": "^7.x",
    "tsup-config": "workspace:*",
    "twin.macro": "3.1.0",
    "typescript": "^5.x"
  },
  "peerDependencies": {
    "@editablejs/editor": "workspace:*",
    "@editablejs/models": "workspace:*"
  },
  "dependencies": {
    "@editablejs/theme": "workspace:*",
    "@emotion/css": "11.11.2",
    "lodash.debounce": "^4.0.8",
    "rezon": "workspace:*",
    "rezon-store": "workspace:*"
  },
  "lint-staged": {
    "src/**/*.{json,ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  }
}
