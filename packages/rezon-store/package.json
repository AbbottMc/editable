{
  "name": "rezon-store",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "vanilla": [
        "./dist/vanilla.d.ts"
      ],
      "shallow": [
        "./dist/shallow.d.ts"
      ],
      "use-sync-external-store": [
        "./dist/use-sync-external-store.d.ts"
      ],
      "use-sync-external-store-with-selector": [
        "./dist/use-sync-external-store-with-selector.d.ts"
      ],
      "use-store-with-equality-fn": [
        "./dist/use-store-with-equality-fn.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./vanilla": {
      "import": {
        "types": "./dist/vanilla.d.mts",
        "default": "./dist/vanilla.mjs"
      },
      "require": {
        "types": "./dist/vanilla.d.ts",
        "default": "./dist/vanilla.js"
      }
    },
    "./shallow": {
      "import": {
        "types": "./dist/shallow.d.mts",
        "default": "./dist/shallow.mjs"
      },
      "require": {
        "types": "./dist/shallow.d.ts",
        "default": "./dist/shallow.js"
      }
    },
    "./use-sync-external-store": {
      "import": {
        "types": "./dist/use-sync-external-store.d.mts",
        "default": "./dist/use-sync-external-store.mjs"
      },
      "require": {
        "types": "./dist/use-sync-external-store.d.ts",
        "default": "./dist/use-sync-external-store.js"
      }
    },
    "./use-sync-external-store-with-selector": {
      "import": {
        "types": "./dist/use-sync-external-store-with-selector.d.mts",
        "default": "./dist/use-sync-external-store-with-selector.mjs"
      },
      "require": {
        "types": "./dist/use-sync-external-store-with-selector.d.ts",
        "default": "./dist/use-sync-external-store-with-selector.js"
      }
    },
    "./use-store-with-equality-fn": {
      "import": {
        "types": "./dist/use-store-with-equality-fn.d.mts",
        "default": "./dist/use-store-with-equality-fn.mjs"
      },
      "require": {
        "types": "./dist/use-store-with-equality-fn.d.ts",
        "default": "./dist/use-store-with-equality-fn.js"
      }
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{json,ts,tsx,js,jsx}\"",
    "clean": "rimraf .turbo && rimraf dist && rimraf node_modules "
  },
  "dependencies": {
    "@editablejs/utils": "workspace:*"
  },
  "peerDependencies": {
    "rezon": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "eslint-config-editable": "workspace:*",
    "jest": "^27.5.1",
    "jest-presets": "workspace:*",
    "prettier-config": "workspace:*",
    "rezon": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^7.x",
    "tsup-config": "workspace:*",
    "typescript": "^5.x"
  },
  "lint-staged": {
    "src/**/*.{json,ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  }
}
