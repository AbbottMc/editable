{
  "name": "@editablejs/utils",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "shallow": [
        "./dist/shallow.d.ts"
      ],
      "deep-equal": [
        "./dist/deep-equal.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./shallow": {
      "import": {
        "types": "./dist/shallow.d.mts",
        "default": "./dist/shallow.mjs"
      },
      "require": {
        "types": "./dist/shallow.d.ts",
        "default": "./dist/shallow.js"
      }
    },
    "./deep-equal": {
      "import": {
        "types": "./dist/deep-equal.d.mts",
        "default": "./dist/deep-equal.mjs"
      },
      "require": {
        "types": "./dist/deep-equal.d.ts",
        "default": "./dist/deep-equal.js"
      }
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{json,ts,tsx,js,jsx}\"",
    "clean": "rimraf .turbo && rimraf dist && rimraf node_modules "
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "eslint-config-editable": "workspace:*",
    "jest": "^27.5.1",
    "jest-presets": "workspace:*",
    "prettier-config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^7.x",
    "tsup-config": "workspace:*",
    "typescript": "^5.x"
  },
  "lint-staged": {
    "src/**/*.{json,ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  }
}
